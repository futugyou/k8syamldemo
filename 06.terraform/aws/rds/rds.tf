resource "aws_db_instance" "onecloud-dev" {
  allocated_storage                     = "20"
  auto_minor_version_upgrade            = "true"
  availability_zone                     = "ap-southeast-1b"
  backup_retention_period               = "7"
  backup_window                         = "21:15-21:45"
  ca_cert_identifier                    = "rds-ca-2019"
  character_set_name                    = "SQL_Latin1_General_CP1_CI_AS"
  copy_tags_to_snapshot                 = "true"
  customer_owned_ip_enabled             = "false"
  db_subnet_group_name                  = var.subnet_group
  deletion_protection                   = "false"
  engine                                = "sqlserver-ex"
  engine_version                        = "14.00.3421.10.v1"
  iam_database_authentication_enabled   = "false"
  identifier                            = var.identifier
  instance_class                        = var.instance_class
  iops                                  = "0"
  license_model                         = "license-included"
  maintenance_window                    = "tue:16:50-tue:17:20"
  max_allocated_storage                 = "1000"
  monitoring_interval                   = "60"
  monitoring_role_arn                   = "arn:aws:iam::383796595686:role/rds-monitoring-role"
  multi_az                              = "false"
  option_group_name                     = "default:sqlserver-ex-14-00"
  parameter_group_name                  = "default.sqlserver-ex-14.0"
  performance_insights_enabled          = "false"
  performance_insights_retention_period = "0"
  port                                  = "1433"
  publicly_accessible                   = "true"
  storage_encrypted                     = "false"
  storage_type                          = "gp2"
  username                              = "admin"
  vpc_security_group_ids                = var.security_group_ids
}
